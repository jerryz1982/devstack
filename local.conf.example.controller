[[local|localrc]]

disable_service n-net
disable_service n-cpu
disable_service ceilometer-acompute
enable_service q-svc q-meta n-novnc

# CHANGEME: customized git repo location
#NEUTRON_REPO=http://10.160.5.251:8080/neutron.git
NEUTRON_REPO=file:///home/stack/neutron
NEUTRON_BRANCH=stable/juno
REQUIREMENTS_REPO=http://github.com/jerryz1982/requirements.git
REQUIREMENTS_BRANCH=test_fortinet

Q_PLUGIN=ml2
ENABLE_TENANT_VLANS=True
ML2_L3_PLUGIN=router_fortinet
Q_ML2_PLUGIN_MECHANISM_DRIVERS='openvswitch,fortinet'
Q_ML2_PLUGIN_VLAN_TYPE_OPTIONS='network_vlan_ranges=physnet1:1000:2999'
Q_ML2_PLUGIN_TYPE_DRIVERS=local,flat,vlan,gre,vxlan
Q_ML2_TENANT_NETWORK_TYPE=vlan
PROVIDER_NETWORK_TYPE=vlan

# generic configuration parameters
# CHANGEME: if needed
DATABASE_PASSWORD=secretsecret
RABBIT_PASSWORD=secretsecret
SERVICE_TOKEN=secretsecret
SERVICE_PASSWORD=secretsecret
ADMIN_PASSWORD=secretsecret

API_RATE_LIMIT="False"
RECLONE="True"

# database configuration parameters

DATABASE_TYPE=mysql

# networking configuration parameters
# CHANGEME: control plane nic
HOST_IP_IFACE=eth1

# logging configuration parameters

LOGFILE=/opt/stack/logs/stack.sh.log
VERBOSE=True
LOG_COLOR=True
SCREEN_LOGDIR=/opt/stack/logs



# configure images provided on glance (manager)

IMAGE_URLS="http://download.cirros-cloud.net/0.3.3/cirros-0.3.3-x86_64-uec.tar.gz"
IMAGE_URLS+=","

enable_service dstat
CEILOMETER_BACKEND=mongodb




[[post-config|/$Q_PLUGIN_CONF_FILE]]
[ml2_fortinet]
# CHANGEME: mgmt ip of fortigate
address = 192.168.121.187
username = admin
password = ''
# CHANGEME: port for tenant network
int_interface = port2
# CHANGEME: port for external network
ext_interface = port3
# whether there is hardware npu
npu_available = False
